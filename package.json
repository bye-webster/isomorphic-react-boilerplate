{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack/webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack/webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/server.bundle.js\"",
    "prod": "npm-run-all prod:*",
    "prod:build-server": "webpack -p --config webpack/webpack.server.js",
    "prod:build-client": "webpack -p --config webpack/webpack.client.js",
    "prod:server": "node build/server.bundle.js"
  },
  "author": "Stefan Lazarevic",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "compression": "1.7.3",
    "concurrently": "3.5.0",
    "critical": "1.3.4",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "express": "4.16.3",
    "express-http-proxy": "1.0.6",
    "extract-text-webpack-plugin": "3.0.2",
    "html-minifier": "3.5.20",
    "html-webpack-plugin": "3.2.0",
    "isomorphic-style-loader": "4.0.0",
    "loadable-components": "2.2.3",
    "lodash": "4.17.11",
    "node-sass": "4.9.3",
    "nodemon": "1.12.0",
    "npm-run-all": "4.1.1",
    "postcss-loader": "3.0.0",
    "react": "16.0.0",
    "react-critical-css": "2.1.0",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "sass-loader": "7.1.0",
    "serialize-javascript": "1.4.0",
    "style-loader": "0.23.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.8.2",
    "webpack-merge": "4.1.0",
    "webpack-node-externals": "1.6.0"
  }
}
