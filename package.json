{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel lint:watch dev:*",
    "dev:build-server": "NODE_ENV=development && webpack --config webpack/webpack.server.js --watch",
    "dev:build-client": "NODE_ENV=development && webpack --config webpack/webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/server.bundle.js\"",
    "prod": "npm-run-all lint prod:*",
    "prod:build-client": "NODE_ENV=production webpack -p --config webpack/webpack.client.js",
    "prod:build-server": "NODE_ENV=production && webpack --config webpack/webpack.server.js",
    "prod:server": "node build/server.bundle.js",
    "lint": "eslint ./src/app/",
    "lint:watch": "esw -w ./src/app/",
    "lint:fix": "eslint ./src/app/ --fix",
    "flow": "flow"
  },
  "author": "Stefan Lazarevic",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-ga": "2.5.3",
    "react-helmet": "5.2.0",
    "react-loadable": "5.5.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "9.1.5",
    "babel-core": "6.26.0",
    "babel-eslint": "10.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-css-modules": "3.4.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "eslint": "^5.6.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "flow-bin": "0.81.0",
    "html-webpack-plugin": "2.30.1",
    "isomorphic-fetch": "2.2.1",
    "isomorphic-style-loader": "4.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "minify-css-string": "^1.0.0",
    "node-sass": "^4.9.3",
    "nodemon": "1.18.4",
    "npm-run-all": "4.1.3",
    "postcss-loader": "3.0.0",
    "sass-loader": "^7.1.0",
    "serialize-javascript": "1.5.0",
    "style-loader": "0.23.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  }
}
